{
  "name": "@codebridger/lib-vue-components",
  "description": "A collection of Vue 3 components based on Tailwind CSS",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "version": "1.16.0",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/vue.cjs.js",
  "module": "./dist/vue.es.js",
  "exports": {
    "./style.css": "./dist/style.css",
    "./shell.ts": "./src/shell/components.ts",
    "./elements.ts": "./src/elements/components.ts",
    "./complex.ts": "./src/complex/components.ts",
    "./toast.ts": "./src/toast.ts",
    "./store.ts": "./src/stores/index.ts",
    "./types.ts": "./src/types/types.ts",
    "./nuxt": {
      "types": "./src/nuxt.ts",
      "require": "./dist/nuxt.cjs.js",
      "import": "./dist/nuxt.es.js"
    },
    ".": {
      "types": "./src/vue.ts",
      "require": "./dist/vue.cjs.js",
      "import": "./dist/vue.es.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build-dev": "vue-tsc --noEmit && vite build --minify false",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@storybook/builder-vite": "^8.4.5",
    "@vueuse/head": "^0.9.7",
    "pinia": "^2.0.22",
    "quill": "^2.0.2",
    "sweetalert2": "^11.15.3",
    "vue": "^3.5.12",
    "vue-collapsed": "^1.3.4",
    "vue-router": "^4.4.5",
    "vue3-perfect-scrollbar": "^1.6.0",
    "vue3-popper": "^1.5.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.2",
    "@rollup/plugin-alias": "^3.1.9",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.0",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@storybook/addon-essentials": "^8.4.5",
    "@storybook/addon-interactions": "^8.4.5",
    "@storybook/addon-links": "^8.4.5",
    "@storybook/addon-onboarding": "^8.4.5",
    "@storybook/blocks": "^8.4.5",
    "@storybook/mdx2-csf": "^1.1.0",
    "@storybook/test": "^8.4.5",
    "@storybook/vue3": "^8.4.5",
    "@storybook/vue3-vite": "^8.4.5",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.7",
    "@vitejs/plugin-vue": "^5.1.4",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "postcss-loader": "^8.1.1",
    "remark-gfm": "^4.0.0",
    "sass": "^1.83.4",
    "semantic-release": "^24.2.0",
    "storybook": "^8.4.5",
    "tailwindcss": "^3.4.17",
    "typescript": "^4.6.4",
    "vite": "4.5.1",
    "vue-tsc": "^0.40.4"
  }
}
