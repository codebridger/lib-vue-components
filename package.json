{
  "name": "@tiny-ideas-ir/lib-vue-components",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vue.cjs.js",
  "module": "./dist/vue.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue.es.js",
      "require": "./dist/vue.cjs.js"
    },
    "./nuxt": {
      "import": "./dist/nuxt.es.js"
    },
    "./style.css": {
      "import": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build-dev": "vue-tsc --noEmit && vite build --minify false",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@storybook/builder-vite": "^8.3.6",
    "@vueuse/head": "^0.9.7",
    "pinia": "^2.0.22",
    "quill": "^2.0.2",
    "vue": "^3.5.12",
    "vue-collapsed": "^1.3.4",
    "vue-router": "^4.4.5",
    "vue3-perfect-scrollbar": "^1.6.0",
    "vue3-popper": "^1.5.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.9.0",
    "@rollup/plugin-alias": "^3.1.9",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.0",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@storybook/addon-essentials": "^8.3.6",
    "@storybook/addon-interactions": "^8.3.6",
    "@storybook/addon-links": "^8.3.6",
    "@storybook/addon-onboarding": "^8.3.6",
    "@storybook/blocks": "^8.3.6",
    "@storybook/test": "^8.3.6",
    "@storybook/vue3": "^8.3.6",
    "@storybook/vue3-vite": "^8.3.6",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.7",
    "@vitejs/plugin-vue": "^5.1.4",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35",
    "semantic-release": "^24.2.0",
    "storybook": "^8.3.6",
    "tailwindcss": "^3.4.1",
    "typescript": "^4.6.4",
    "vite": "5.0.0",
    "vue-tsc": "^0.40.4"
  }
